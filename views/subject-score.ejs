<%- include layout/header %>
  <br>
  <h3>Add Score Students Data</h3>
  <h4>Mata pelajaran: <%= subject.subject_name %></h4>
  <form class="form-default" action="/subject/giveScore?idStudent=<%=student.id%>&idSubject=<%=subject.id%>" method="post">
    <div class="form-group">
      <label> First name</label><br>
      <input type="text" name="first_name" value="<%=student.first_name%>" disabled><br>
    </div>
    <div class="form-group">
      <label> Last name</label><br>
      <input type="text" name="last_name" value="<%= student.last_name%>" disabled><br>
    </div>
    <div class="form-group">
      <label> Email</label><br>
      <input type="text" name="email" value="<%= student.email%>" disabled  ><br>
    </div>
    <div class="form-group">
      <label> Nilai</label><br>
      <input type="number" name="score" value=""><br>
    </div>
    <button type="submit" class="btn btn-primary">Simpan nilai</button>
  </form>
  <br>
  <a href="/subject/enrolledStudents?id=<%=subject.id%>"><button class="btn btn-danger" type="button">Cancel</button></a>
<%- include layout/footer %>
