<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Table Subject</title>
  </head>
  <body>
    <h2>Data Table Subject</h2>
    <table border="1">
      <thead>
        <tr>
          <% dTableHeader.forEach(x=>{%>
          <th><%= x%></th>
          <% }) %>
        </tr>
      </thead>
      <tbody>
        <% dSubject.forEach(x=>{%>
          <tr>
            <td><%= x.id %></td>
            <td><%= x.subject_name %></td>
            <td><%= x.createdAt %></td>
            <td><%= x.updatedAt %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <br>
    <br>
    <a href="\"><button type="button"> Back to Index</button></a>

  </body>
</html>
