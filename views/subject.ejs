<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Table Subject</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
  </head>
  <body>
    <h2>Data Table Subject</h2>
    <table class="table">
      <thead class="thead-default">
        <tr>
          <td>Id</td>
          <td>Subject Name</td>
          <td>Teachers</td>
        </tr>
      </thead>
      <tbody class="">
        <% data.forEach(x=>{%>
          <tr>
            <td><%= x.id %></td>
            <td><%= x.subject_name %></td>
            <td>
                <%-x.teachers.map((z,x=0)=>{ x++; return `${x}. `+z.first_name+`<br>`})%>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <br>
    <br>
    <a href="\"><button type="button"> Back to Index</button></a>

  </body>
</html>
